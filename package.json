{
  "name": "streamlined-pdf-ticket-processor",
  "version": "2.0.0",
  "description": "1000% accuracy PDF train ticket extraction system with direct text processing and Supabase integration",
  "type": "module",
  "main": "src/pdf-pipeline.js",
  "scripts": {
    "start": "node src/pdf-pipeline.js",
    "dev": "nodemon src/pdf-pipeline.js",
    "pipeline": "node src/pdf-pipeline.js",
    "pipeline:debug": "node src/pdf-pipeline.js --debug",
    "pipeline:no-import": "node src/pdf-pipeline.js --no-import",
    "server": "node src/api/server.js",
    "server:dev": "nodemon src/api/server.js",
    "supabase": "node src/supabase-import.js",
    "db-import": "node src/supabase-import.js import",
    "db-stats": "node src/supabase-import.js stats",
    "db-test": "node src/supabase-import.js test",
    "db-search": "node src/supabase-import.js search",
    "db-timeline": "node src/supabase-import.js timeline",
    "db-enhanced-timeline": "node src/supabase-import.js enhanced-timeline",
    "db-validate-journeys": "node src/supabase-import.js validate-journeys",
    "db-migrate": "node src/supabase/run-migrations.js",
    "db-migrate-apply": "node src/supabase/run-migrations.js apply",
    "db-migrate-status": "node src/supabase/run-migrations.js status",
    "db-migrate-cleanup": "node src/supabase/run-migrations.js cleanup",
    "db-migrate-test": "node src/supabase/run-migrations.js test",
    "env-check": "node -e \"import('./src/env-validator.js').then(m => m.validateEnvironment())\"",
    "env-test": "node -e \"import('./src/env-validator.js').then(m => m.testConnection())\"",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "health-check": "./scripts/health-check.sh local",
    "health-check-prod": "./scripts/health-check.sh production",
    "deploy": "./scripts/deploy.sh all",
    "deploy-backend": "./scripts/deploy.sh backend",
    "deploy-frontend": "./scripts/deploy.sh frontend"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.56.1",
    "chalk": "^5.3.0",
    "dotenv": "^17.2.1",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "pg": "^8.16.3",
    "express": "^4.21.1",
    "cors": "^2.8.5",
    "multer": "^2.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "pdf-extraction",
    "train-tickets",
    "supabase",
    "irctc",
    "ticket-processing",
    "direct-text-extraction",
    "1000-accuracy",
    "streamlined-pipeline",
    "web-interface",
    "upload-processing"
  ],
  "author": "",
  "license": "MIT"
}
